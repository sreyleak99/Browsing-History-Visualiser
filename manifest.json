{
    "manifest_version": 3,
    "name": "Browsing History Visualiser",
    "version": "1.0.0",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "history",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "image/history.png"
    }
  }
  
  